{
  "expo": {
    "name": "KGFlex",
    "slug": "GYM_Management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kgflex.png",
    "scheme": "gymmanagement",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.kgflex.gymmanagement_22092025",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-barcode-scanner",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "576b8144-ccbe-4ecd-8b95-ce142b86f039"
      }
    }
  }
}
